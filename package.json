{
  "name": "react-lazy-imports",
  "version": "1.1.0",
  "description": "A dynamic bundles splliting helper for react/webpack applications.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel dev --out-dir dist",
    "lint": "eslint src tests --fix --ignore-pattern .gitignore",
    "test": "mocha --compilers js:babel-register tests",
    "test:coverage": "nyc npm run test",
    "codecov": "codecov"
  },
  "pre-commit": [
