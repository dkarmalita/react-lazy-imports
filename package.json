    }
  ],
  "author": "Dmitriy Karmalita <altmine@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkarmalita/react-lazy-imports.git"
  },
  "bugs": {
    "url": "https://github.com/dkarmalita/react-lazy-imports/issues"
  },
  "homepage": "https://github.com/dkarmalita/react-lazy-imports#readme",
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-preset-babili": "0.0.11",
    "babel-preset-latest": "6.22.0",
    "babel-register": "6.22.0",
    "codecov": "1.0.1",
    "eslint": "3.15.0",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "pre-commit": "1.2.2"
  }
